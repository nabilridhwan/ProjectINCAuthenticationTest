{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": ["airbnb-base"],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "quotes": ["error", "double"],
        "linebreak-style": "off",
        "indent": ["error", 4, { "SwitchCase": 1 }],
        "object-curly-newline": "off",
        "max-len": "off",
        "comma-dangle": "off"
    }
}
